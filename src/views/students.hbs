<div class="container">
  <table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">First Name</th>
        <th scope="col">Second Name</th>
        <th scope="col">Actions</th>
      </tr>
    </thead>
    <tbody>
      {{#each students}}
      <tr>
        <th scope="row">1</th>
        <td>{{this.firstName}}</td>
        <td>{{this.lastName}}</td>
        <td><a href="/students/edit/:id">Edit</a></td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>